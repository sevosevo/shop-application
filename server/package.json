  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node -r dotenv/config ./dist/server.js",
    "dev-server": "nodemon -r dotenv/config --watch src -e ts --exec ts-node src/server.ts",
    "dev-client": "npm start --prefix ../client",
    "dev-both": "concurrently \"npm run dev-server\" \"npm run dev-client\" ",
    "heroku-postbuild": "npm run build && npm i --prefix ../client && npm run build --prefix ../client"
  },
